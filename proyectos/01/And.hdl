 /**
 * And gate:
 * if ((a == 1) and (b == 1)) out = 1, else out = 0 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
    //// Usando la compuerta Nand, a partir de negar ambas entradas se 
    //// puede obtener la compuerta Or
}